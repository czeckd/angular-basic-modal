{
  "name": "angular2-simple-modal",
  "description": "Angular 2 provider to add and display simple modal dialog windows.",
  "version": "2.1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/czeckd/angular2-simple-modal.git"
  },
  "author": "David Czeck",
  "license": "MIT",
  "scripts": {
    "start": "tsc && concurrently \"tsc -w\" \"lite-server\" ",
